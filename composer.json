{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "bensampo/laravel-enum": "^5.3.1",
        "fruitcake/laravel-cors": "^2.2.0",
        "guzzlehttp/guzzle": "^7.4.5",
        "laravel/framework": "^9.18.0",
        "laravel/passport": "^10.4.1",
        "laravel/sanctum": "^2.15.1",
        "laravel/scout": "^9.4.9",
        "laravel/tinker": "^2.7.2",
        "predis/predis": "^1.1.10",
        "sentry/sentry-laravel": "^2.12.1",
        "spatie/flysystem-dropbox": "^2.0.6",
        "spatie/laravel-activitylog": "^4.5.3",
        "spatie/laravel-backup": "^8.1.2",
        "spatie/laravel-permission": "^5.5.4"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12.3",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.19.0",
        "laravel/sail": "^1.14.11",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.2.1",
        "phpunit/phpunit": "^9.5.21",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
